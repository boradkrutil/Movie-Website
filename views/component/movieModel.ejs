
  <div class="modal fade" id="exampleModal" tabindex="-1" aria-labelledby="exampleModalLabel" aria-hidden="true">
    <div class="modal-dialog modal-dialog-centered modal-fullscreen"   >
      <div class="modal-content">
        <div class="modal-header">
          <h5 class="modal-title" id="exampleModalLabel">Feature Images</h5>
          <button type="button" class="btn-close" data-bs-dismiss="modal" aria-label="Close"></button>
        </div>
        <div class="modal-body">
            <div class="d-flex flex-wrap" style="display: grid; ">
          <% let i =0; while (i<gallary.length) {%>
                <img id="<%=gallary[i]._id%>" src="<%=gallary[i].path%>" onclick="selectFetureImage({
                    _id:'<%=gallary[i]._id%>',
                    name:'<%=gallary[i].name%>',
                    path:'<%=gallary[i].path%>',
                }, this)" alt="img" height="441.59px" width="298.08px" style="border: 1px solid black; margin: 10px 6px;" />
          <%i++ } %>
          
            </div>
        </div>
        <div class="modal-footer">
          <button type="button" class="btn btn-secondary" data-bs-dismiss="modal">Close</button>
          <button type="button" class="btn btn-primary" data-bs-dismiss="modal" onclick="imageSelecthander()">Save changes</button>
        </div>
      </div>
    </div>
  </div>