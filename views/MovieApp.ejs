    <meta name="description" content="">
    <meta name="viewport" content="width=device-width, initial-scale=1">

    <link rel="shortcut icon" type="image/x-icon" href="images/favicon.png">
    <!-- Place favicon.ico in the root directory -->

    <!-- CSS here -->
    <link rel="stylesheet" href="../public/css2/bootstrap.min.css">
    <link rel="stylesheet" href="../public/css2/animate.min.css">
    <link rel="stylesheet" href="../public/css2/magnific-popup.css">
    <link rel="stylesheet" href="../public/css2/fontawesome-all.min.css">
    <link rel="stylesheet" href="../public/css2/owl.carousel.min.css">
    <link rel="stylesheet" href="../public/css2/flaticon.css">
